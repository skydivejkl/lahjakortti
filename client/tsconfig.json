{
    "compilerOptions": {
        "sourceMap": true,
        "module": "es6",
        "target": "es5",
        "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "noUnusedLocals": true,
        "moduleResolution": "node",
        "noUnusedParameters": true
    }
}
